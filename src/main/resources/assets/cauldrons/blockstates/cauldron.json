{
  "multipart": [
    {
      "apply": {
        "model": "cauldrons:block/cauldron_bottom"
      },
      "when": {
        "downwards_connected": "false"
      }
    },
    {
      "apply": {
        "model": "cauldrons:block/cauldron_bottom_side"
      },
      "when": {
        "northwards_connected": "false"
      }
    },
    {
      "apply": {
        "model": "cauldrons:block/cauldron_bottom_column",
        "y": 90
      },
      "when": {
        "eastwards_connected": "false",
        "northwards_connected": "true"
      }
    },
    {
      "apply": {
        "model": "cauldrons:block/cauldron_bottom_column"
      },
      "when": {
        "northwards_connected": "true",
        "westwards_connected": "false"
      }
    },
    {
      "apply": {
        "model": "cauldrons:block/cauldron_bottom_side",
        "y": 180
      },
      "when": {
        "southwards_connected": "false"
      }
    },
    {
      "apply": {
        "model": "cauldrons:block/cauldron_bottom_side",
        "y": 90
      },
      "when": {
        "eastwards_connected": "false"
      }
    },
    {
      "apply": {
        "model": "cauldrons:block/cauldron_bottom_side",
        "y": 270
      },
      "when": {
        "westwards_connected": "false"
      }
    }
  ]
}